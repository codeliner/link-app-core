{
    "name": "prooph/prooph-link-app-core-module",
    "description": "Prooph LINK module that provides default functionality for a prooph LINK application",
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/prooph/ProophLinkAppCoreModule",
    "authors": [
        {
            "name": "Alexander Miertsch",
            "email": "contact@prooph.de",
            "homepage": "http://www.prooph.de/"
        }
    ],
    "keywords": [
        "prooph",
        "prooph LINK",
        "module",
        "ZF2"
    ],
    "require": {
        "php": ">=5.5",
        "zendframework/zend-loader" : "~2.3",
        "zendframework/zend-servicemanager" : "~2.3",
        "zendframework/zend-modulemanager" : "~2.3",
        "zendframework/zend-mvc" : "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "zendframework/zend-form" : "~2.3",
        "zendframework/zend-view" : "~2.3",
        "zendframework/zend-log" : "~2.3",
        "zendframework/zend-config" : "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Prooph\\Link\\Application\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProophTest\\Link\\Application\\": "tests/"
        }
    }
}
