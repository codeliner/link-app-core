{
    "name": "prooph/link-app-core",
    "description": "Prooph LINK module that provides default functionality for a prooph LINK application",
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/prooph/link-app-core",
    "authors": [
        {
            "name": "Alexander Miertsch",
            "email": "contact@prooph.de",
            "homepage": "http://www.prooph.de/"
        }
    ],
    "keywords": [
        "prooph",
        "prooph LINK",
        "module",
        "ZF2"
    ],
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework": "~2.3",
        "prooph/processing" : "-0.4",
        "prooph/proophessor" : "-0.3",
        "zfcampus/zf-api-problem": "~1.0",
        "zfcampus/zf-content-negotiation": "~1.0",
        "prooph/event-store-doctrine-adapter": "~1.0",
        "doctrine/doctrine-orm-module": "~0.8.0",
        "rwoverdijk/assetmanager": "~1.4",
        "phly/mustache" : "~1.2",
        "mathiasverraes/classfunctions" : "~1.0",
        "beberlei/assert": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "Prooph\\Link\\Application\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProophTest\\Link\\Application\\": "tests/"
        }
    }
}
